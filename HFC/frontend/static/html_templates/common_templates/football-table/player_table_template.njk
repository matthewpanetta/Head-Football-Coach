              <td>{{player.player_team_season.ratings.blocking.pass_block}}</td>
              <td>{{player.player_team_season.ratings.blocking.run_block}}</td>
              <td class='col-group'>{{player.player_team_season.ratings.blocking.impact_block}}</td>
            {% endif %}

            {% if column_controls.Ratings.athleticism.shown %}
              <!-- THROWING RATINGS -->
              <td>{{player.player_team_season.ratings.athleticism.speed}}</td>
              <td>{{player.player_team_season.ratings.athleticism.acceleration}}</td>
              <td>{{player.player_team_season.ratings.athleticism.agility}}</td>
              <td>{{player.player_team_season.ratings.athleticism.jumping}}</td>
              <td>{{player.player_team_season.ratings.athleticism.strength}}</td>
              <td>{{player.player_team_season.ratings.athleticism.stamina}}</td>
              <td class='col-group'>{{player.player_team_season.ratings.athleticism.injury}}</td>
            {% endif %}

            {% if column_controls.Ratings.defense.shown %}
              <!-- THROWING RATINGS -->
              <td>{{player.player_team_season.ratings.defense.tackle}}</td>
              <td>{{player.player_team_season.ratings.defense.hit_power}}</td>
              <td>{{player.player_team_season.ratings.defense.block_shedding}}</td>
              <td>{{player.player_team_season.ratings.defense.pursuit}}</td>
              <td>{{player.player_team_season.ratings.defense.play_recognition}}</td>
              <td>{{player.player_team_season.ratings.defense.man_coverage}}</td>
              <td>{{player.player_team_season.ratings.defense.zone_coverage}}</td>
              <td>{{player.player_team_season.ratings.defense.press}}</td>
              <td class='col-group'>{{player.player_team_season.ratings.defense.pass_rush}}</td>
            {% endif %}

            {% if column_controls.Ratings.kicking.shown %}
              <!-- THROWING RATINGS -->
              <td>{{player.player_team_season.ratings.kicking.kick_power}}</td>
              <td class='col-group'>{{player.player_team_season.ratings.kicking.kick_accuracy}}</td>
            {% endif %}


          </tr>

        {% endfor %}
      </tbody>
    </table>
    
  </div>
  {% if pagination.max_pages > 1%}
      <div class="football-table-pagination card-footer" style="float: right;">
        {% if pagination.current_page > 1%}
          <button class='btn btn-light football-table-pagination-button font12' pagination-action="1">
            First
          </button>
          <button class='btn btn-light football-table-pagination-button font12 margin-right-12' pagination-action="{{pagination.current_page - 1}}">
            Prev
          </button>
        {% endif %}
        {% for page_number in pagination.available_page_navigation%}
          <button class='btn btn-light football-table-pagination-button font12 
                        {% if page_number == pagination.current_page %} selected {% endif %}' 
                  pagination-action="{{page_number}}">
            {{page_number}}
          </button>
        {% endfor %}
        {% if pagination.current_page < pagination.max_pages%}
          <button class='btn btn-light football-table-pagination-button font12 margin-left-12' pagination-action="{{pagination.current_page + 1}}">
            Next
          </button>
          <button class='btn btn-light football-table-pagination-button font12' pagination-action="{{pagination.max_pages}}">
            Last 
            <span class="font10">({{pagination.max_pages}})</span>
          </button>
        {% endif %}
      </div>
    {% endif %}
</div>